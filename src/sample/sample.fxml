<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.web.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<BorderPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <center>
      <WebView fx:id="webView" prefHeight="262.0" prefWidth="600.0" BorderPane.alignment="TOP_CENTER" />
   </center>
   <bottom>
      <BorderPane prefHeight="46.0" prefWidth="800.0" BorderPane.alignment="CENTER">
          <top>
              <Label fx:id="labelLaatsteRegel" text="Here comes the lines" BorderPane.alignment="CENTER" />
          </top>
         <bottom>
            <Label fx:id="label" text="00:00:00" BorderPane.alignment="CENTER" />

         </bottom>
         <center>
            <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <center>
                  <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                     <children>
                        <Button fx:id="selectButton" mnemonicParsing="false" onAction="#selectPressed" text="Select File" />
                        <Button fx:id="playButton" disable="true" mnemonicParsing="false" onAction="#playPressed" text="Play/Pause" />
                     </children>
                  </HBox>
               </center>
            </BorderPane>
         </center>
      </BorderPane>
   </bottom>
</BorderPane>
